<Project Sdk="Microsoft.NET.Sdk">

<PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <Authors>jakub.gredzicki@gmail.com</Authors>
    <Title>AppLauncher</Title>
    <Product>AppLauncher</Product>
    <Description>App allowing to combine different programs executables to launch and manage from one place</Description>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<Deterministic>false</Deterministic>
	<ApplicationIcon>icon.ico</ApplicationIcon>
</PropertyGroup>

<ItemGroup>
  <Content Include="icon.ico" />
</ItemGroup>

<ItemGroup>
  <None Update="ReadMe.md">
    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
  </None>
</ItemGroup>

</Project>
